<h1>Importă angajamente</h1>

<p><%= link_to 'Înapoi la registrul de angajamente', commitments_path %></p>

<%= render partial: 'flashes' %>

<%= bootstrap_form_with url: import_upload_commitments_path, method: :post do |form| %>
  <%= form.file_field :file, accept: 'application/vnd.openxmlformats-officedocument.spreadsheetml.sheet' %>

  <%= form.submit 'Importă' %>
<% end %>
